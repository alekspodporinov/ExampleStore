@model Store.WebUI.Models.GamesListViewModel

@{
    ViewBag.Title = "Товары";
}

@foreach (var p in @Model.Games)
{
    <div>
        <h3>@p.Name</h3>
        <p>@p.Description</p>
        <h4>@p.Price.ToString("# руб")</h4>
    </div>
}

<div>
    @MyHelpers.PageLinks(Html, Model.PagingInfo)
</div>